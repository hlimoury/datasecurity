    <!-- About Start -->
    <div >
<section id="apropos">
    <div class="container-fluid py-5 wow fadeInUp" data-wow-delay="0.1s">
        <div class="container">
            <div class="row gx-5">
                <div class="col-lg-5 mb-5 mb-lg-0" style="min-height: 500px;">
                    <div class="position-relative h-100">
                        <img class="position-absolute w-100 h-100 rounded wow zoomIn" data-wow-delay="0.3s" src="img/about.png" style="object-fit: cover;">
                    </div>
                </div>
                <div class="col-lg-7">
                    <div class="mb-4">
                        <h5 class="text-primary text-uppercase" style="letter-spacing: 5px;">A PROPOS</h5>
                        <h1 class="display-5 mb-0" style="text-align: left;">DATA SECURITY est un prestataire au service de votre sécurité qui intervient dans tout LE MAROC.</h1>
                    </div>
                    <h4 class="text-body fst-italic mb-4">DATA SECURITY dispose d'une équipe ayant une expérience de plus de 16 ans dans le domaine de la sécurité électronique .</h4>
                    <p class="mb-4">DATA SECURITY  pour objet de permettre à ses futurs clients d'être garanti de prestations irréprochables;
                        L'objectif final étant de permettre à ses clients de trouver un partenaire en sécurité électronique.</p>
                    <div class="row g-3">
                        <div class="col-sm-6 wow zoomIn" data-wow-delay="0.6s">
                            <div class="bg-primary d-flex flex-column justify-content-center text-center border-bottom border-5 border-secondary rounded p-3" style="height: 200px;">
                                <i class="fa fa-star fa-4x text-white mb-4"></i>
                                <h4 class="text-white mb-0">16 Years Experience</h4>
                            </div>
                        </div>
                        <div class="col-sm-6 wow zoomIn" data-wow-delay="0.9s">
                            <div class="bg-secondary d-flex flex-column justify-content-center text-center border-bottom border-5 border-primary rounded p-3" style="height: 200px;">
                                <i class="fa fa-award fa-4x text-white mb-4"></i>
                                <h4 class="text-white mb-0">Ton Premier Choix</h4>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</section>
    <!-- About End -->
    

    <!-- Services Start -->
<section id="services">
    <div class="container-fluid py-5 wow fadeInUp" data-wow-delay="0.1s">
        <div class="container">
            <div class="text-center mx-auto mb-5" style="max-width: 600px;">
                <h5 class="text-primary text-uppercase" style="letter-spacing: 5px;">Services</h5>
                <h1 class="display-5 mb-0">Nos Excellents Services De Sécurité</h1>
            </div>
            <div class="row g-5">


                <!-- //// insert DATA ///// -->

                <% services.forEach(service => { %>
                    <div class="col-lg-4 col-md-6 wow zoomIn" data-wow-delay="0.3s">
                      <div class="service-item bg-light border-bottom border-5 border-primary rounded">
                        <div class="position-relative p-5">
                          <!-- Image -->
                          <img src="<%= service.image %>" alt="<%= service.title %>" style="width: 100%; height: auto; ">
                          <!-- Title -->
                          <h3 class="mb-3 text-secondary"><%= service.title %></h3>
                          <!-- Truncated Summary -->
                          <h5 class="mb-3"><%= truncateText(service.summary, 100) %></h5>
                          <!-- Link to Products for the Service -->
                          <a href="/service/<%= service._id %>/products" class="btn btn-secondary">Voir Les Produits <i class="bi bi-arrow-right ms-2"></i></a>
                        </div>
                      </div>
                    </div>
                  <% }) %>
                
            </div>
        </div>
    </div>
</section>
    <!-- Services End -->
  </div>



<!-- Brands Carousel Start -->
<div class="container-fluid py-5">
    <div class="container">
      <h2 class="mb-4 text-center">Nos Marques</h2>
      <div class="swiper-container brands-carousel">
        <div class="swiper-wrapper">
          <% brandImages.forEach(brandImage => { %>
            <div class="swiper-slide">
              <div class="brand-item">
                <% if (brandImage.image) { %>
                  <img src="<%= brandImage.image %>" alt="<%= brandImage.name %>" class="img-fluid">
                <% } %>
              </div>
            </div>
          <% }) %>
        </div>
        <div class="swiper-pagination"></div>
        <!-- Optional navigation buttons -->
        <div class="swiper-button-next"></div>
        <div class="swiper-button-prev"></div>
      </div>
    </div>
  </div>
  
  <!-- Brands Carousel End -->
  

  

    <!-- Nouveaux Produits Start -->
    
<section id="nouveaux-produits">
    <div class="container-fluid py-5 wow fadeInUp" data-wow-delay="0.1s">
      <div class="container">
        <div class="text-center mx-auto mb-5" style="max-width: 600px;">
          <h5 class="text-primary text-uppercase" style="letter-spacing: 5px;">Nouveaux Produits</h5>
          <h1 class="display-5 mb-0">Découvrez Nos Derniers Produits</h1>
        </div>
  
        <!-- Swiper Container -->
        <div class="swiper-container produits-slider">
          <div class="swiper-wrapper">
            <% newProducts.forEach(product => { %>
              <div class="swiper-slide">
                <div class="product-item">
                  <!-- Image du produit -->
                  <img src="<%= product.image %>" alt="<%= product.name %>" class="img-fluid">
                  <!-- Icône Nouveau -->
                  <span class="new-icon">Nouveau</span>
                  <!-- Informations du produit -->
                  <div class="product-info mt-3">
                    <h5 class="mb-1"><%= product.name %></h5>
                    <!-- Lien vers les détails du produit -->
                    <a href="/product/<%= product._id %>" class="btn btn-secondary mt-2">Voir Détails</a>
                  </div>
                </div>
              </div>
            <% }) %>
           
            
          </div>
  
          <!-- Pagination -->
          <div class="swiper-pagination"></div>
  
          <!-- Navigation -->
          <div class="swiper-button-prev"></div>
          <div class="swiper-button-next"></div>
        </div>
      </div>
    </div>
  </section>
  <!-- Nouveaux Produits End -->
  
     





  
    <section class="client py-5">
        <div class="container">
            <div class="row justify-content-center">
                <div class="col-12 text-center">
