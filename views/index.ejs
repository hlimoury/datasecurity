    <!-- Services Start -->
<section id="services">
    <div class="container-fluid py-5 wow fadeInUp" data-wow-delay="0.1s">
        <div class="container">
            <div class="text-center mx-auto mb-5" style="max-width: 600px;">
                <h5 class="text-primary text-uppercase" style="letter-spacing: 5px;">Services</h5>
                <h1 class="display-5 mb-0">Nos Excellents Services De Sécurité</h1>
            </div>
            <div class="row g-5">


                <!-- //// insert DATA ///// -->

                <% services.forEach(service => { %>
                    <div class="col-lg-4 col-md-6 wow zoomIn" data-wow-delay="0.3s">
                      <div class="service-item bg-light border-bottom border-5 border-primary rounded">
                        <div class="position-relative p-5">
                          <!-- Image -->
                          <img src="<%= service.image %>" alt="<%= service.title %>" style="width: 100%; height: auto; ">
                          <!-- Title -->
                          <h3 class="mb-3 text-secondary"><%= service.title %></h3>
                          <!-- Truncated Summary -->
                          <h5 class="mb-3"><%= truncateText(service.summary, 100) %></h5>
                          <!-- Link to Products for the Service -->
                          <a href="/service/<%= service._id %>/products" class="btn btn-secondary">View Products <i class="bi bi-arrow-right ms-2"></i></a>
                        </div>
                      </div>
                    </div>
                  <% }) %>
                
            </div>
        </div>
    </div>
</section>
    <!-- Services End -->


  

