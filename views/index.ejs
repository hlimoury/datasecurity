                        <div class="project-item">
                            <% if (project.image) { %>
                                <img src="<%= project.image %>" alt="<%= project.title %>" class="img-fluid">
                            <% } %>
                            <h3><%= project.title %></h3>
                            <p><%= project.phrase %></p>
                        </div>
                    </div>
                <% }) %>
                
            </div>
            <!-- Pagination and Navigation -->
            <div class="swiper-pagination"></div>
            <div class="swiper-button-next"></div>
            <div class="swiper-button-prev"></div>
        </div>
    </div>
</div>

